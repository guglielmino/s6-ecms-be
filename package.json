{
  "name": "server",
  "version": "2.3.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "babel-node app.js",
    "lint": "eslint ./ --cache --ignore-pattern .gitignore --ignore-pattern *.spec.js --ignore-pattern *.e2e.js",
    "dev": "nodemon --exec npm run start -- app.js",
    "test": "mocha --compilers js:babel-core/register --recursive 'src/**/*.spec.js'",
    "test:e2e": "mocha --compilers js:babel-core/register --recursive 'src/**/*.e2e.js'",
    "test:watch": "npm run test -- --watch",
    "validate-commit": "validate-commit-msg",
    "postinstall": "./scripts/postinstall.sh",
    "precommit": "npm run lint && npm run test",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert",
        "release",
        "ci"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "autoFix": false,
      "scope": {
        "required": false
      }
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "dependencies": {
    "auth0": "^2.6.0",
    "body-parser": "^1.17.0",
    "cors": "^2.8.1",
    "dotenv": "4.0.0",
    "excel-export": "^0.5.1",
    "express": "^4.14.0",
    "express-jwt": "^5.1.0",
    "express-validation": "^1.0.2",
    "fast-json-patch": "^1.2.0",
    "joi": "^10.4.1",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.20",
    "pubnub": "^4.3.3",
    "react": "^15.4.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.2",
    "rxjs": "^5.0.2",
    "sentry": "^0.1.2",
    "socket.io": "^1.7.2",
    "socketio-jwt": "^4.5.0",
    "swagger-jsdoc": "^1.9.1",
    "swagger-tools": "^0.10.1",
    "winston": "^2.3.0",
    "winston-sentry": "^0.1.5"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.3.1",
    "eslint": "^3.16.1",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jasmine": "^2.2.0",
    "mocha": "^3.2.0",
    "mockery": "2.1.0",
    "node-mocks-http": "^1.6.4",
    "nodemon": "^1.11.0",
    "rewire": "^2.5.2",
    "sinon": "^1.17.7",
    "supertest": "^3.0.0",
    "validate-commit-msg": "^2.12.1"
  }
}
